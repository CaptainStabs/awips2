<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.raytheon.viz.inHydroCommonActionSet">
         <with variable="activeContexts">
            <iterate operator="or">
               <or>
                  <equals
                        value="com.raytheon.viz.hydro.HydroActionSet">
                  </equals>
                  <equals
                        value="com.raytheon.viz.MPEActionSet">
                  </equals>
               </or>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.raytheon.viz.hydrocommon.actions.pan"
            name="Pan">
         <commandParameter
               id="direction"
               name="direction"
               optional="false">
         </commandParameter>
      </command>
      <command
            id="com.raytheon.viz.hydrocommon.actions.zoom"
            name="Zoom">
         <commandParameter
               id="direction"
               name="direction"
               optional="false">
         </commandParameter>
      </command>
      <command
            id="com.raytheon.viz.hydrocommon.actions.setProjection"
            name="Set Projection">
         <commandParameter
               id="projection"
               name="projection"
               optional="false">
         </commandParameter>
         <commandParameter
               id="prefix"
               name="prefix"
               optional="false">
         </commandParameter>
      </command>
      <command
            id="com.raytheon.viz.hydrocommon.actions.fontsize"
            name="Set Font">
         <commandParameter
               id="size"
               name="size"
               optional="true">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.raytheon.viz.hydrocommon.actions.Pan"
            commandId="com.raytheon.viz.hydrocommon.actions.pan">
      </handler>
      <handler
            class="com.raytheon.viz.hydrocommon.actions.Zoom"
            commandId="com.raytheon.viz.hydrocommon.actions.zoom">
      </handler>
      <handler
            class="com.raytheon.viz.hydrocommon.actions.SetProjection"
            commandId="com.raytheon.viz.hydrocommon.actions.setProjection">
      </handler>
      <handler
            class="com.raytheon.viz.hydrocommon.actions.FontAction"
            commandId="com.raytheon.viz.hydrocommon.actions.fontsize">
      </handler>
   </extension>
</plugin>
