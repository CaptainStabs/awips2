    <!-- TOWRdocs Header
         GOES-R Center/Test Position Sectors description file for Level2 products.
    -->
    <!-- TOWRdocs Description
         This file specifies descriptions for the Center/Test position sectors.
         Description files are used to decode the GOES-R
         netcdf imagery and Level2 products. When there are new products a description file
         must be added. This file specifies products for the Test/Center location. When description
         files are added or modified, edex must be restarted for the changes to be reflected.
    -->
    <!-- TOWRdocs Status
         This is a new file not included in the current baseline. The baseline does not contain 
         Center/Test position Level2 descriptions. This file is similar to the baseline 
         GOES-East-Sectors.xml and GOES-West-Sectors.xml files.
    -->
    <!-- TOWRdocs POC
         Lee Byerle
    -->
<goesrProductDescriptions>
  <default>
    <matches>
        <match pattern="GOES-Test">
            <attribute name="orbital_slot" />
        </match>
    </matches>
  </default>

    <description>
    <matches>
        <match  pattern="CONUS">
            <attribute name="scene_id" />
        </match>
    </matches>
    <sectorID>
        <value value="TCONUS" />
    </sectorID>
  </description>
  
  <description>
    <matches>
        <match  pattern="Full Disk">
            <attribute name="scene_id" />
        </match>
    </matches>
    <sectorID>
        <value value="TFD" />
    </sectorID>
  </description>
  
  <description>
    <matches>
      <match pattern="OR_ABI-L2-\w{3,5}M1-M[34]_G\d\d_s\d{14}_e\d{14}_c\d{14}.nc">
        <attribute name="dataset_name" />
      </match>
      <match pattern="Mesoscale">
        <attribute name="scene_id" />
      </match>
    </matches>
    <sectorID>
        <value value="TMESO-1" />
    </sectorID>
  </description>

  <description>
    <matches>
      <match pattern="OR_ABI-L2-\w{3,5}M2-M[34]_G\d\d_s\d{14}_e\d{14}_c\d{14}.nc">
        <attribute name="dataset_name" />
      </match>
      <match  pattern="Mesoscale">
          <attribute name="scene_id" />
      </match>
    </matches>
    <sectorID>
        <value value="TMESO-2" />
    </sectorID>
  </description>
</goesrProductDescriptions>
