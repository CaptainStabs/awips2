<requestPatterns xmlns:ns2="group">
	<regex>^TI[SR]</regex>
    <!-- This pattern is intended to match standardized Level 2 filenames, it will not be used over LDM -->
    <regex>^OR_ABI-L2-\w{3,5}(C|F|M1|M2)-M[34]_G\d\d_s\d{14}_e\d{14}_c\d{14}.nc$</regex>
</requestPatterns>
